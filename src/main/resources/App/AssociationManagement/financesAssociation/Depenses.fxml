<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.AssociationManagement.Controller.financesAssociation.DepensesController">
   <children>
        <VBox fx:id="vbox1">
            <children>
                <MenuBar fx:id="menuBar" nodeOrientation="RIGHT_TO_LEFT" prefHeight="25.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
                    <menus>
                        <Menu fx:id="menu1" mnemonicParsing="false" text="Autres">
                            <items>
                                <MenuItem fx:id="menuItem1" mnemonicParsing="false" text="Quitter" />
                            </items>
                        </Menu>
                        <Menu fx:id="menu2" mnemonicParsing="false" text="Notifications">
                            <items>
                                <MenuItem fx:id="menuItem2" mnemonicParsing="false" text="Voir les notifications" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </VBox>
        <VBox fx:id="vbox2" prefHeight="80.0" prefWidth="600.0">
            <children>
                <Pane fx:id="pane1" prefHeight="66.0" prefWidth="600.0">
                    <children>
                        <Button fx:id="retourButtonDepenses" mnemonicParsing="false" onAction="#onButtonRetourClick" text="Retour" />
                        <Label fx:id="titreLabelDepenses" layoutX="274.0" layoutY="25.0" text="Dépenses" />
                    </children>
                </Pane>
            </children>
        </VBox>
      <VBox prefHeight="327.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" spacing="50.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="defraiementVisitesButton" mnemonicParsing="false" onAction="#onButtonDefraiementVisitesClick" prefHeight="48.0" prefWidth="200.0" style="-fx-wrap-text: true;" text="Défraiement des visites d'arbres faites par les membres" textAlignment="CENTER" />
                  <Button fx:id="paiementFacturesButton" mnemonicParsing="false" onAction="#onButtonPaiementFacturesClick" prefHeight="48.0" prefWidth="200.0" style="-fx-wrap-text: true;" text="Paiement des factures" textAlignment="CENTER" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</VBox>
