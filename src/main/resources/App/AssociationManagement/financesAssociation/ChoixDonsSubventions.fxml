<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@FinancesStyles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.AssociationManagement.Controller.financesAssociation.ChoixDonsSubventionsController">
   <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="400.0" spacing="50.0" stylesheets="@FinancesStyles.css">
         <children>
            <Button fx:id="donButton" mnemonicParsing="false" onAction="#onButtonDonClick" prefHeight="50.0" prefWidth="100.0" text="Don" />
            <Button fx:id="subventionButton" mnemonicParsing="false" onAction="#onButtonSubventionClick" prefHeight="50.0" prefWidth="107.0" text="Subvention" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="400.0">
         <children>
            <Button fx:id="annulerButton" mnemonicParsing="false" onAction="#onButtonAnnulerClick" prefHeight="50.0" prefWidth="100.0" text="Annuler" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
